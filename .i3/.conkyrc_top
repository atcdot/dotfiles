{"full_text": " ♪ ${exec amixer get Master |grep 'Front Left' | grep -o "[[:digit:]]*%"} ", "color":"\#75dcff"},
{ "full_text" : " bat ${battery_percent}% " , "color" : "\#87ff73" },
{"full_text": " ${time %a %d %b} ${time %H:%M:%S} ","color": "\#ffa24e"}],

